{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\usappointment.js",
      "args": [
        "-r",
        "<region>",
        "-d",
        "<threshold-date>",
        "-u",
        "<username or email>",
        "-p",
        "<password>",
        "-a",
        "<application Id>",
        "-c",
        "<Consulate Id (Optional)>",
        "-t",
        "<retry timeout (seconds)>",
        "-n",
        "<Pushover User Key>"
      ]
    }
  ]
}
